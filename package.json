{
  "name": "start-javascript",
  "version": "1.0.0",
  "description": "Typescript boilerplate starter project.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack.config.ts --open --env dev",
    "build": "webpack --config webpack.config.ts --env prod",
    "analyze": "webpack --config webpack.config.ts --env analyze",
    "test": "jest",
    "test:watch": "jest --watch",
    "server-ts": "tsc ./src/server/server.ts && json-server ./src/server/server.js",
    "server-json": "json-server ./src/server/server.json",
    "debug": "ts-node-dev --respawn --transpile-only"
  },
  "author": "Cosmin Vasilache",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/mini-css-extract-plugin": "^1.4.3",
    "@types/webpack": "^4.41.32",
    "@types/webpack-bundle-analyzer": "^3.9.4",
    "@types/webpack-dev-server": "^3.11.6",
    "css-loader": "^5.2.7",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "json-server": "^0.16.3",
    "mini-css-extract-plugin": "^1.6.2",
    "node-sass": "^5.0.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2",
    "webpack": "^5.64.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^5.8.0"
  },
  "optionalDependencies": {
    "windows-build-tools": "^5.2.2"
  }
}
